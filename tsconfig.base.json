{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["WebWorker", "ES2021"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "declaration": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true
  }
}
